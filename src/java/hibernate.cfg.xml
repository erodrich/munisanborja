<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQL5Dialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/munisanborja</property>
    <property name="hibernate.connection.username">root</property>
    <property name="hibernate.connection.password">admin</property>
    <property name="hibernate.show_sql">true</property>
    <property name="hibernate.format_sql">true</property>
    <property name="hibernate.current_session_context_class">thread</property>
    
    <mapping class="com.munisanborja.sys.model.entities.EstadoTramite" />
    <mapping class="com.munisanborja.sys.model.entities.CostoBeneficio" />
    <mapping class="com.munisanborja.sys.model.entities.CostoEfectividad" />
    <mapping class="com.munisanborja.sys.model.entities.MontoInversionTotal" />
    <mapping class="com.munisanborja.sys.model.entities.Requerimiento" />
    <mapping class="com.munisanborja.sys.model.entities.Ubicacion" />
    <mapping class="com.munisanborja.sys.model.entities.UnidadFormuladora" />
  </session-factory>
</hibernate-configuration>
